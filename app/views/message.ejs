<!doctype html>
<html>
<head>
    <title>Recycle</title>
    <link rel="stylesheet" type="text/css" href="./css/main.css">
</head>
<body>
<div id="container">
    <div id="nav">
        <ul>
            <li><a href="/">Home</a></li>
            <li><a href="/profile"><%= user.username %></a></li>
            <li><a href="/logout">Logout</a></li>
        </ul>
    </div>

    <div id="content">
    	<% if (message.length > 0) { %>

            <div id="msg"><%= message %></div>

        <% } else { %>

            <div id="msg">No message</div>

        <% } %>

    </div>
</div> 
</body>
</html>
